<view id="searchbar" class="cu-bar search bg-white box" style="box-sizing: border-box">
    <view class="search-form round">
        <text class="cuIcon-search"></text>
        <input type="text" disabled="{{true}}"
               bindtap='searchbtnclick' placeholder="搜索 {{placeholder}} 房源（^ - ^）"
               confirm-type="search"/>
    </view>
    <view class="action" bindtap="CitySelect">
        <text>{{placeholder}}</text>
        <text class="cuIcon-triangledownfill"></text>
    </view>
</view>
<swiper-banner id="swiperbanner" wx:if="{{banners.length>0}}" imgs_list="{{banners}}"  swiper_style="card-swiper"></swiper-banner>
<view class="icon-list">
    <view class="icon-item" wx:for="{{icon_list}}" wx:key bindtap="ImageClick" data-path="{{item.path}}">
        <image src="{{item.icon_url}}"></image>
        <text class="text-center text-grey margin-top-xs text-xs">{{item.name}}</text>
    </view>
</view>
<view class="feed-list">
    <view class="feed-left">
        <image data-path="{{feed_conf.one.path}}" class="feed-left" src="{{feed_conf.one.url}}" bindtap="ImageClick"
        ></image>
    </view>
    <view class="feed-right">
        <view class="r-top">
            <image data-path="{{feed_conf.two.path}}" src="{{feed_conf.two.url}}" bindtap="ImageClick"></image>
        </view>
        <view class="r-bottom">
            <image data-path="{{feed_conf.three.path}}" src="{{feed_conf.three.url}}" bindtap="ImageClick"></image>
            <image data-path="{{feed_conf.four.path}}" src="{{feed_conf.four.url}}" bindtap="ImageClick"></image>
        </view>
    </view>
</view>
